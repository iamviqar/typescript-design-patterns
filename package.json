{
  "name": "typescript-design-patterns",
  "version": "1.0.0",
  "description": "Comprehensive implementation of all 23 GoF Design Patterns in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "demo:creational": "ts-node src/demos/creationalDemo.ts",
    "demo:structural": "ts-node src/demos/structuralDemo.ts",
    "demo:behavioral": "ts-node src/demos/behavioralDemo.ts"
  },
  "keywords": [
    "design-patterns",
    "typescript",
    "gof",
    "software-engineering",
    "patterns",
    "creational",
    "structural",
    "behavioral"
  ],
  "author": "Design Patterns for Humans",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
